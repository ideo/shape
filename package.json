  "scripts": {
    "styleguide": "yarn styleguidist server",
    "test": "jest --watch",
    "lint": "./node_modules/.bin/eslint .",
    "cypress": "./node_modules/.bin/cypress open",
    "cypress-ci": "./node_modules/.bin/cypress run"
  },
  "jest": {
    "testURL": "http://localhost",
    "roots": [
      "<rootDir>/__tests__",
      "<rootDir>/app/javascript"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "setupTestFrameworkScriptFile": "./__js_test_config/setup.js",
    "modulePaths": [
      "__js_test_config"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|svg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "/config/webpack/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "resetMocks": false,
    "automock": false,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/javascript/src",
      "__tests__",
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
